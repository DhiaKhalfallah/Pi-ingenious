<StackPane fx:id="parentContainer" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pi.controllers.StartController">
   <children>
      <AnchorPane id="AnchorPane" fx:id="container" prefHeight="720.0" prefWidth="1280.0" styleClass="cyan-bg" stylesheets="@../css/Stylesheet.css">
         <children>
            <ImageView fitHeight="720.0" fitWidth="1367.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/startpage.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="720.0" fitWidth="1028.0" layoutX="252.0" pickOnBounds="true" preserveRatio="true">
