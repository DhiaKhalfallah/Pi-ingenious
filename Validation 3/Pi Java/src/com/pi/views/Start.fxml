            </ImageView>
            <JFXButton fx:id="getStartedButton" buttonType="RAISED" layoutX="95.0" layoutY="359.0" onAction="#open_login" prefHeight="76.0" prefWidth="307.0" ripplerFill="WHITE" styleClass="get-started-btn" text="GET STARTED" textFill="WHITE">
